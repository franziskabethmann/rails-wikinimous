<div class="container">
  <h1>Wikinimous</h1>
  <ul class="list-group">
    <% @articles.each do |article| %>
      <li class="list-group-item"><%= link_to article.title, article_path(article) %>
        <p> <%= article.content %></p>
      </li>
    <% end %>
  </ul>
  <br>
  <p><%= link_to "+ Add article", new_article_path %></p>
</div>




